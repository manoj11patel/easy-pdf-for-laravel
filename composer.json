
{
  "name": "manoj11patel/laravel-tcpdf",
  "description": "TCPDF library for Laravel for all the latest versions",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
  	"laravel",
  	"tcpdf",
  	"pdf"
  ],
  "authors": [
	{
	  "name": "Manoj Patel",
	  "email": "mp.inviable@gmail.com"
	}
  ],
  "require": {
	   "illuminate/support": "^6.0|^7.0|^8.0",
	   "tecnickcom/tcpdf": "6.2.*|6.3.*|6.4.*|dev-main"
  },
  "autoload": {
  	"psr-4": {
  	  "Manoj\\TCPDF\\": "src/"
  	}
  },
  "extra": {
      "laravel": {
          "providers": [
              "Manoj\\TCPDF\\ServiceProvider"
          ],
          "aliases": {
              "PDF": "Manoj\\TCPDF\\Facades\\TCPDF"
          }
      }
  }
}
